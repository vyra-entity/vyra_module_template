{
  "name": "v2-modulemanager-frontend",
  "version": "2.0.0",
  "description": "V2 ModuleManager TypeScript Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@primevue/themes": "^4.4.1",
    "axios": "^1.6.0",
    "chart.js": "^4.5.0",
    "pinia": "^2.1.7",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "vue": "^3.4.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^4.0.18",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.17.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^20.3.7",
    "prettier": "^3.0.3",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^2.1.0"
  }
}
