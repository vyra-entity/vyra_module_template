{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "default": {
            "()": "logging.Formatter",
            "format": "%(asctime)s - %(levelname)-8s - %(name)s - %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        },
        "access": {
            "()": "logging.Formatter",
            "format": "%(asctime)s - %(levelname)-8s - %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "default": {
            "formatter": "default",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        },
        "access": {
            "formatter": "access",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "uvicorn": {
            "handlers": ["default", "file"],
            "propagate": false
        },
        "uvicorn.error": {
            "handlers": ["default", "file"],
            "propagate": false
        },
        "uvicorn.access": {
            "handlers": ["access", "file"],
            "propagate": false
        },
        "fastapi": {
            "handlers": ["default", "file"],
            "propagate": false
        },
        "": {
            "handlers": ["default", "file"],
            "propagate": false
        }
    },
    "root": {
        "level": "NOTSET",
        "handlers": ["default", "file"]
    }
}
